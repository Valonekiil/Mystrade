render_mode blend_mix;

uniform vec2 scale = vec2(1.000000, 1.000000);
uniform vec2 speed;
uniform sampler2D Texture_main : source_color, repeat_enable;
uniform sampler2D Texture_add : source_color;
uniform sampler2D Texture_multiply : source_color;
uniform sampler2D Texture_Subtract : source_color;
uniform sampler2D Texture_color_1D : source_color;



void fragment() {
// Vector2Parameter:10
	vec2 n_out10p0 = scale;


// UVFunc:9
	vec2 n_in9p2 = vec2(0.00000, 0.00000);
	vec2 n_out9p0 = (UV - n_in9p2) * n_out10p0 + n_in9p2;


// Input:6
	float n_out6p0 = TIME;


// Vector2Parameter:7
	vec2 n_out7p0 = speed;


// VectorOp:8
	vec2 n_out8p0 = vec2(n_out6p0) * n_out7p0;


// UVFunc:5
	vec2 n_in5p1 = vec2(1.00000, 1.00000);
	vec2 n_out5p0 = n_out8p0 * n_in5p1 + n_out9p0;


	vec4 n_out3p0;
// Texture2D:3
	n_out3p0 = texture(Texture_main, n_out5p0);
	float n_out3p1 = n_out3p0.r;


	vec4 n_out11p0;
// Texture2D:11
	n_out11p0 = texture(Texture_add, UV);
	float n_out11p1 = n_out11p0.r;


// FloatOp:14
	float n_out14p0 = n_out3p1 + n_out11p1;


	vec4 n_out15p0;
// Texture2D:15
	n_out15p0 = texture(Texture_multiply, UV);
	float n_out15p1 = n_out15p0.r;


// FloatOp:21
	float n_out21p0 = n_out14p0 * n_out15p1;


	vec4 n_out18p0;
// Texture2D:18
	n_out18p0 = texture(Texture_Subtract, UV);
	float n_out18p1 = n_out18p0.r;


// FloatOp:22
	float n_out22p0 = n_out21p0 - n_out18p1;


	vec4 n_out20p0;
// Texture2D:20
	n_out20p0 = texture(Texture_color_1D, vec2(n_out22p0));
	float n_out20p4 = n_out20p0.a;


// Output:0
	COLOR.rgb = vec3(n_out20p0.xyz);
	COLOR.a = n_out20p4;


}
