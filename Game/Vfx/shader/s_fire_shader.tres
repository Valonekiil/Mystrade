	vec2 n_out17p0 = n_out20p0 * n_in17p1 + n_out21p0;


	vec4 n_out3p0;
// Texture2D:3
	n_out3p0 = texture(texture_main, n_out17p0);
	float n_out3p1 = n_out3p0.r;


	vec4 n_out5p0;
// Texture2D:5
	n_out5p0 = texture(texture_add, UV);
	float n_out5p1 = n_out5p0.r;


// FloatOp:23
