	vec4 n_out12p0;
// Texture2D:12
	n_out12p0 = texture(texture_color_1D, vec2(n_out25p0));
	float n_out12p4 = n_out12p0.a;


// Output:0
	COLOR.rgb = vec3(n_out12p0.xyz);
	COLOR.a = n_out12p4;


}
