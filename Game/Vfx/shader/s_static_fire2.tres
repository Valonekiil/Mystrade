	vec2 n_out11p0 = n_out13p0 * n_in11p1 + n_out14p0;


	vec4 n_out6p0;
// Texture2D:6
	n_out6p0 = texture(texture_main, n_out11p0);
	float n_out6p1 = n_out6p0.r;


	vec4 n_out7p0;
// Texture2D:7
