	vec4 n_out21p0;
// Texture2D:21
	n_out21p0 = texture(texture_substract, UV);
	float n_out21p1 = n_out21p0.r;


// FloatOp:24
	float n_out24p0 = n_out17p0 - n_out21p1;


	vec4 n_out23p0;
// Texture2D:23
	n_out23p0 = texture(texture_color_1D, vec2(n_out24p0));
	float n_out23p4 = n_out23p0.a;


// Output:0
	COLOR.rgb = vec3(n_out23p0.xyz);
	COLOR.a = n_out23p4;


}
